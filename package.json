    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "knip": "^5.18.2",
    "postcss": "^8.4.38",
