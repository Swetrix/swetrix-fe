{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "deploy": "flyctl deploy --remote-only",
    "import-env": "node fly_import_env.js --env-file .env",
    "dev:staging": "cross-env STAGING=true remix dev",
    "start": "remix-serve ./build/index.js",
    "start:staging": "cross-env STAGING=true remix-serve ./build/index.js",
    "typecheck": "tsc",
    "lint": "eslint app",
    "lint:fix": "eslint app --fix",
    "lint:tsc": "tsc --noEmit",
    "format": "prettier --check --ignore-unknown ./app ./public",
    "format:fix": "prettier --write --ignore-unknown ./app ./public",
    "unused:find": "knip"
  },
  "dependencies": {
    "@blaumaus/react-alert": "^7.0.7",
    "@headlessui/react": "^2.0.4",
    "@heroicons/react": "^2.1.3",
    "@nivo/core": "^0.87.0",
    "@nivo/sankey": "^0.87.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@remix-run/serve": "^2.9.2",
    "@remix-run/v1-route-convention": "^0.1.4",
    "@swetrix/sdk": "^1.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "axios": "^1.7.2",
    "axios-auth-refresh": "^3.3.6",
    "billboard.js": "^3.11.3",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "dangerously-set-html-content": "^1.1.0",
    "dayjs": "^1.11.11",
    "debug": "^4.3.4",
    "file-saver": "^2.0.5",
    "flatpickr": "^4.6.13",
    "i18n-iso-countries": "^7.11.2",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.5.2",
    "isbot": "^5.1.7",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "marked": "^12.0.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-flagkit": "^2.0.4",
    "react-flatpickr": "^3.10.13",
    "react-hotkeys-hook": "^4.5.0",
    "react-i18next": "^14.1.2",
    "react-outside-click-handler": "^1.3.0",
    "react-qr-code": "^2.0.13",
    "react-redux": "^9.1.2",
    "react-type-animation": "^3.2.0",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0",
    "remix-i18next": "^6.1.0",
    "remix-sitemap": "^3.2.0",
    "remix-utils": "^7.6.0",
    "spacetime": "^7.6.0",
    "swetrix": "^3.1.1",
    "timezone-soft": "^1.5.2"
  },
  "eslintConfig": {
    "extends": [
      "@remix-run/eslint-config",
      "eslint:recommended"
    ],
    "globals": {
      "JSX": "readonly"
    },
    "plugins": [
      "lodash",
      "react-hooks"
    ],
    "rules": {
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "lodash/prefer-lodash-method": "warn",
      "comma-dangle": [
        "warn",
        "always-multiline"
      ],
      "@typescript-eslint/consistent-type-imports": "off",
      "jsx-a11y/anchor-has-content": "off",
      "no-unused-vars": "off",
      "react-hooks/rules-of-hooks": "error",
      "semi": [
        "error",
        "never"
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "app"
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "env": {
      "browser": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.2",
    "@remix-run/eslint-config": "^2.9.2",
    "@tailwindcss/typography": "^0.5.13",
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-flatpickr": "^3.8.11",
    "@types/redux-mock-store": "^1.0.6",
    "@typescript-eslint/parser": "^7.10.0",
    "@typescript-eslint/typescript-estree": "^7.10.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "knip": "^5.18.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20"
  }
}
