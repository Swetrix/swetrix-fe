{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "dev:staging": "cross-env STAGING=true remix vite:dev",
    "start:staging": "cross-env STAGING=true remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "lint": "eslint app",
