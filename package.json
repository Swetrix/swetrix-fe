{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "dev:staging": "cross-env STAGING=true remix dev",
    "start": "cross-env NODE_ENV=production netlify dev",
    "start:staging": "cross-env STAGING=true NODE_ENV=production netlify dev",
    "test": "jest --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "test:ci": "CI=true npm run test",
    "typecheck": "tsc",
    "lint": "eslint app",
    "lint:fix": "eslint app --fix",
    "lint:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@blaumaus/react-alert": "^7.0.5",
    "@netlify/functions": "^1.4.0",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@nivo/core": "^0.83.0",
    "@nivo/sankey": "^0.83.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@remix-run/node": "^1.17.1",
    "@remix-run/netlify": "^1.17.1",
    "@remix-run/react": "^1.17.1",
    "@remix-run/serve": "^1.17.1",
    "@swetrix/sdk": "^1.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "axios": "^1.4.0",
    "axios-auth-refresh": "^3.3.6",
    "babel-jest": "^29.5.0",
    "billboard.js": "^3.8.2",
    "clsx": "^1.2.1",
    "d3": "^7.8.5",
    "dayjs": "^1.11.8",
    "debug": "^4.3.4",
    "dom-to-image": "^2.6.0",
    "file-saver": "^2.0.5",
    "flatpickr": "^4.6.13",
    "i18n-iso-countries": "^7.6.0",
    "i18next": "^22.5.0",
    "i18next-browser-languagedetector": "^7.0.2",
    "i18next-fs-backend": "^2.1.2",
    "i18next-http-backend": "^2.2.1",
    "isbot": "^3.6.10",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flagkit": "^2.0.4",
    "react-flatpickr": "^3.10.13",
    "react-i18next": "^12.3.1",
    "react-outside-click-handler": "^1.3.0",
    "react-qr-code": "^2.0.11",
    "react-redux": "^8.1.0",
    "react-select": "^5.7.3",
    "react-timezone-select": "^2.1.2",
    "react-type-animation": "^3.1.0",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3",
    "remix-i18next": "^5.1.1",
    "remix-sitemap": "^2.1.0",
    "spacetime": "^7.4.4",
    "swetrix": "^2.2.1",
    "timezone-soft": "^1.4.1",
    "ts-jest": "^29.1.0"
  },
  "eslintConfig": {
    "extends": [
      "@remix-run/eslint-config",
      "@remix-run/eslint-config/jest-testing-library",
      "airbnb",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "globals": {
      "JSX": "readonly"
    },
    "plugins": [
      "lodash",
      "react-hooks"
    ],
    "rules": {
      "arrow-body-style": "off",
      "arrow-parens": "off",
      "import/no-cycle": "off",
      "import/no-named-as-default": "off",
      "import/no-named-as-default-member": "off",
      "import/prefer-default-export": "off",
      "import/no-unresolved": "off",
      "no-param-reassign": "off",
      "@typescript-eslint/camelcase": "off",
      "camelcase": "off",
      "import/extensions": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/control-has-associated-label": "off",
      "jsx-a11y/label-has-for": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off",
      "testing-library/prefer-user-event": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "linebreak-style": "off",
      "lines-between-class-members": "off",
      "lodash/prefer-lodash-method": "warn",
      "import/newline-after-import": "off",
      "@typescript-eslint/consistent-type-imports": "off",
      "consistent-return": "off",
      "max-len": "off",
      "no-console": "off",
      "no-bitwise": "off",
      "no-continue": "off",
      "no-empty-function": "off",
      "no-nested-ternary": "off",
      "no-plusplus": "off",
      "no-underscore-dangle": "off",
      "no-unused-vars": "off",
      "react/display-name": "warn",
      "react/jsx-filename-extension": [
        "warn",
        {
          "extensions": [
            ".js",
            ".jsx",
            ".tsx",
            ".ts"
          ]
        }
      ],
      "react/jsx-props-no-spreading": "off",
      "react/prefer-stateless-function": "off",
      "react/prop-types": "off",
      "react/state-in-constructor": "off",
      "react/static-property-placement": "off",
      "react-hooks/rules-of-hooks": "error",
      "react/react-in-jsx-scope": "off",
      "react-hooks/exhaustive-deps": "warn",
      "react/function-component-definition": "off",
      "semi": [
        "error",
        "never"
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "app"
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "env": {
      "browser": true,
      "jest": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@remix-run/dev": "^1.17.1",
    "@remix-run/eslint-config": "^1.17.1",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/react": "^14.0.0",
    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.1",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/react-flatpickr": "^3.8.8",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-mock-store": "^1.0.3",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/parser": "^5.59.11",
    "@typescript-eslint/typescript-estree": "^5.59.11",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.24",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=14"
  }
}
